{
  "name": "automated-api-testing",
  "version": "1.0.0",
  "scripts": {
    "test": "playwright test",
    "format": "prettier --write \"src/**/*.ts\" \"config/**/*.ts\" \"*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"config/**/*.ts\" \"*.ts\"",
    "lint": "eslint \"src/**/*.ts\" \"config/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"config/**/*.ts\" --fix"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@playwright/test": "^1.56.0",
    "@types/node": "^24.7.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "prettier": "^3.7.4"
  }
}
